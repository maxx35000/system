<favorites xmlns:launcher="http://schemas.android.com/apk/res/com.sec.android.emergencylauncher" >    <home>        <!-- Permission[wxyz] -->        <!--             w : Always enabled.             x : Use AlarmManager.             y : Use mobile data when screen off.             z : Use default App permission, Don't kill if turn off screen.        -->        <!-- SBrowser -->        <appshortcut            launcher:className="com.sec.android.app.sbrowser.SBrowserMainActivity"            launcher:packageName="com.sec.android.app.sbrowser"            launcher:permission="1001" />        <!-- ChatOn -->        <appshortcut            launcher:className="com.sec.chaton.HomeActivity"            launcher:packageName="com.sec.chaton"            launcher:permission="0000" />		<!-- WeChat -->        <appshortcut            launcher:className="com.tencent.mm.ui.LauncherUI"            launcher:packageName="com.tencent.mm"            launcher:permission="0000" />        <!-- Calculator -->        <appshortcut            launcher:className="com.sec.android.app.popupcalculator.Calculator"            launcher:packageName="com.sec.android.app.popupcalculator"            launcher:permission="0000" />        <!-- Clock -->        <appshortcut            launcher:className="com.sec.android.app.clockpackage.ClockPackage"            launcher:packageName="com.sec.android.app.clockpackage"            launcher:permission="0101" />        <!-- Memo -->        <appshortcut            launcher:className="com.samsung.android.app.memo.Main"            launcher:packageName="com.samsung.android.app.memo"            launcher:permission="0001" />        <!-- Voice recorder -->        <appshortcut            launcher:className="com.sec.android.app.voicenote.main.VNMainActivity"            launcher:packageName="com.sec.android.app.voicenote"            launcher:permission="0101" />    </home></favorites>